---
import type { CollectionEntry } from "astro:content";
interface Props {
  work: CollectionEntry<"works">;
}
const { work } = Astro.props;
const { Content } = await work.render();
---

<div>
  {work.data.title}
  <div class="prose prose-2xl overflow-visible relative mb-20">
    <Content />
  </div>
</div>
